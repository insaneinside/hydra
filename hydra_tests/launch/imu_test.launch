<?xml version="1.0" encoding="utf-8"?>
<launch>
  <arg name="device" default="/dev/ttyUSB0"/>
  <param name="/imu/publish_rate" value="50"/>
  <param name="/imu/update_rate" value="100"/>
  <param name="/imu/samples_per_message" value="2"/>
  <node name="serial" pkg="rosserial_python" type="serial_node.py" args="$(arg device)"/>
</launch>
